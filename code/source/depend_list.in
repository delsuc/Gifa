# link 
gifa :  libgif.a
	$L -o gifa gifa.o libgif.a $(LDFLAG)

# different versions
gifa_small : gifa
	cp gifa gifa_small

gifa_norm : gifa
	cp gifa gifa_norm

gifa_big : gifa
	cp gifa gifa_big

#library, this one is to make it from scratch
library : $(OBJECTS)
	if test -f libgif.a; then rm -f libgif.a ; fi
	ar cru libgif.a `lorder $(OBJECTS) plotlibpost.o | tsort  `
	$(ARCHI) \
	cd $$objdir ; $$ranlib libgif.a

libgif.a : $(OBJECTS)
	ar r libgif.a $?
	$(ARCHI) \
	cd $$objdir ; $$ranlib libgif.a

